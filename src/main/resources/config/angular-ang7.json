			"imports": [
				{
					"packageName": "@angular/core",
					"importClasses": "Input"
				}
			],
			"declarations": [
				{
					"annotation": "Input",
					"variableName": "errorMsg",
					"datatype": "string"
				},
				{
					"annotation": "Input",
					"variableName": "displayError",
					"datatype": "boolean"
				},
				{
					"annotation": "Input",
					"variableName": "errorTypes",
					"datatype": "Object"
				}
			]
		},
